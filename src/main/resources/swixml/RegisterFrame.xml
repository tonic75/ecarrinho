<?xml version="1.0" encoding="UTF-8"?>
<panel background="255,255,255">
	<formlayout colspecs="5px, l:60px, f:200px,15px" rowspecs="5px,f:20px,5px,f:20px,5px,f:20px,5px,f:20px,5px,f:20px,5px,f:20px,5px,f:20px,5px,f:20px,10px,25px" />
	<label text="Nome:">
		<cellconstraints gridX="2" gridY="2" />
	</label>
	<textfield id="nameField">
		<cellconstraints gridX="3" gridY="2" />
	</textfield>
	
	<label text="Endereço:">
		<cellconstraints gridX="2" gridY="4" />
	</label>
	<textfield id="adsressField">
		<cellconstraints gridX="3" gridY="4" />
	</textfield>
	
	<label text="CPF:">
		<cellconstraints gridX="2" gridY="6" />
	</label>
	<textfield id="cpfField">
		<cellconstraints gridX="3" gridY="6" />
	</textfield>
	
	<label text="Cartão:">
		<cellconstraints gridX="2" gridY="8" />
	</label>
	<textfield id="cardField">
		<cellconstraints gridX="3" gridY="8" />
	</textfield>
	
	<label text="Agência:">
		<cellconstraints gridX="2" gridY="10" />
	</label>
	<textfield id="agencyField">
		<cellconstraints gridX="3" gridY="10" />
	</textfield>
	
	<label text="Conta:">
		<cellconstraints gridX="2" gridY="12" />
	</label>
	<textfield id="accField">
		<cellconstraints gridX="3" gridY="12" />
	</textfield>
	
	<label text="Usuário:">
		<cellconstraints gridX="2" gridY="14" />
	</label>
	<textfield id="userField">
		<cellconstraints gridX="3" gridY="14" />
	</textfield>
	
	<label text="Senha:">
		<cellconstraints gridX="2" gridY="16" />
	</label>
	<passwordfield id="passField">
		<cellconstraints gridX="3" gridY="16" />
	</passwordfield>
	
	<panel background="255,255,255">
		<formlayout colspecs="95px,10px,95px" rowspecs="25px" />
		<cellconstraints gridX="3" gridY="18"  />
		<button text="Registrar" action="register">
			<cellconstraints gridX="1" gridY="1" />
		</button>
		<button text="Cancelar" action="cancel">
			<cellconstraints gridX="3" gridY="1" />
		</button>
	</panel>
	
	
</panel>